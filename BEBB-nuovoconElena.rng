                     <zeroOrMore>
                        <choice>
                           <ref name="tei_model.ptrLike"/>
                           <ref name="tei_idno"/>
                           <ref name="tei_editor"/>
                           <ref name="tei_respStmt"/>
                        </choice>
                     </zeroOrMore>
                  </group>
                  <group>
                     <oneOrMore>
                        <choice>
