                        </choice>
                     </oneOrMore>
                     <zeroOrMore>
                        <choice>
                           <ref name="tei_author"/>
                           <ref name="tei_editor"/>
                           <ref name="tei_respStmt"/>
                        </choice>
                     </zeroOrMore>
                  </group>
                  <ref name="tei_idno"/>
               </choice>
            </optional>
            <zeroOrMore>
               <ref name="tei_availability"/>
